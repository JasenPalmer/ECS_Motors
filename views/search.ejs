<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width = device-width, initial-scale = 1">

		<link rel="stylesheet" type="text/css" href="css/bootstrap.css">
		<link rel="stylesheet" type="text/css" href="css/style.css">
		<script src="js/jquery.min.js"></script>
		<script src="js/bootstrap.min.js"></script>
		<script src="navbar.js"></script>
		<script src="search.js"></script>


		<title><%= title %></title>
	</head>
	<body>

		<% include ../views/partials/navbar %>
		<% include ../views/partials/login-register %>

		<!-- List of cars -->
		<div style="padding-top: 50px;" class="container">
			<div class="strike" style="padding-bottom: 50px;">
				<span>
					<h1>Search Results</h1>
				</span>
			</div>
				<% for (var i = 0; i < data.rows.length; i++){ %>
					<div class="col-sm-4">
						<% var image = "/img/cars-for-sale/" + JSON.stringify(data.rows[i].id) + ".jpg"%>
						<img id= <%= JSON.stringify(data.rows[i].id) %> class="img-responsive carClick" src=<%= image %> alt= <%= JSON.stringify(data.rows[i].brand) %>>
					</div>
				<% } %>
		</div>

		<!-- Car info modal -->
		<div id="carInfo" class="modal fade" role="dialog">
		</div>


		<!-- Footer -->
		<footer class="footer">
			<div class="container">
			</div>
		</footer>
	</body>
</html>
